{
  "name": "anisaronno/laravel-self-updater",
  "description": "Laravel Self Updater is a package that helps you to update your Laravel application automatically.",
  "version": "0.3.2",
  "license": "MIT",
  "keywords": [
    "laravel",
    "updater",
    "package",
    "self-updater"
  ],
  "authors": [
    {
      "name": "MD. Anichur Rahaman",
      "email": "contact@anichur.com",
      "homepage": "https://anichur.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/anisAronno/laravel-self-updater",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "ext-zip": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8.0.0",
    "phpunit/phpunit": "^10.4"
  },
  "autoload": {
    "psr-4": {
      "AnisAronno\\LaravelSelfUpdater\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnisAronno\\LaravelSelfUpdater\\Tests\\": "tests",
      "Workbench\\App\\": "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter",
    "format": "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist.php --allow-risky=yes",
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve --ansi"
    ],
    "lint": [
      "@php vendor/bin/phpstan analyse --verbose --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AnisAronno\\LaravelSelfUpdater\\LaravelSelfUpdaterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
